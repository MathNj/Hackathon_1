# Authentication Service Environment Variables
# Copy this file to ../.env in the project root

# Database (Neon Postgres)
DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# Authentication
AUTH_SECRET=your_random_secret_key_here_min_32_chars
AUTH_URL=http://localhost:3001
AUTH_PORT=3001

# CORS (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000,https://your-github-pages-url.github.io

# Environment
NODE_ENV=development
