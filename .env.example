# Environment Configuration
# Copy this file to .env and replace with your actual API keys

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Configuration (for Gemini compatibility)
OPENAI_API_BASE=https://generativelanguage.googleapis.com/v1beta/openai/
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (Neon Postgres)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require&channel_binding=require

# Qdrant Configuration
QDRANT_URL=https://your-qdrant-instance.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here

# Better-Auth Configuration
AUTH_SECRET=your_auth_secret_here_use_openssl_rand_hex_32
AUTH_URL=http://localhost:3001
AUTH_PORT=3001

# Application Configuration
ENVIRONMENT=development
DEBUG=true
CORS_ORIGINS=http://localhost:3000,https://your-github-pages-url.github.io
